<!DOCTYPE html>
<html lang="en">
   <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <meta name="description" content="">
      <meta name="author" content="">
      <link rel="icon" type="image/png" href="/img/fav.png">
      <title>Nexhub Connect</title>
      <!-- Slick Slider -->
      <link rel="stylesheet" type="text/css" href="/vendor/slick/slick.min.css"/>
      <link rel="stylesheet" type="text/css" href="/vendor/slick/slick-theme.min.css"/>
      <!-- Feather Icon-->
      <link href="/vendor/icons/feather.css" rel="stylesheet" type="text/css">
      <!-- Bootstrap core CSS -->
      <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
      <!-- Custom styles for this template -->
      <link href="/css/style.css" rel="stylesheet">
   </head>
   <body>
      <!-- Navigation -->
      <nav class="navbar navbar-expand navbar-light bg-white osahan-nav-top p-0">
         <div class="container">
            <img src="/img/logo2.png" alt="" width="50px" height="50px">
            </a>
            <ul class="navbar-nav ml-auto d-flex align-items-center">
               <li class="nav-item">
                  <a class="nav-link" href="/"><i class="feather-home mr-2"></i><span class="d-none d-lg-inline">Home</span></a>
               </li>

               <li class="nav-item">
                  <a class="nav-link" href="/connect"><i class="feather-users mr-2"></i><span class="d-none d-lg-inline">Connect</span></a>
               </li>
               {{#if user}}
               <li class="nav-item dropdown no-arrow ml-1 osahan-profile-dropdown">
                  <a class="nav-link dropdown-toggle pr-0" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <img class="img-profile rounded-circle" src="/images/profile/{{user.photo}}">
                  </a>
                  <!-- Dropdown - User Information -->
                  <div class="dropdown-menu dropdown-menu-right shadow-sm">
                     <div class="p-3 d-flex align-items-center">
                        <div class="dropdown-list-image mr-3">
                           <img class="rounded-circle" src="/images/profile/{{user.photo}}" alt="">
                           <div class="status-indicator bg-success"></div>
                        </div>
                        <div class="font-weight-bold">
                           <div class="text-truncate">{{user.name}}</div>
                           <div class="small text-gray-500">{{user.nama}}</div>
                        </div>
                     </div>
                     <a class="dropdown-item" href="/auth/logout"><i class="feather-log-out mr-1"></i> Logout</a>
                  </div>
               </li>
               {{else}}
                    <li class="nav-item">
                        <a class="nav-link" href="/login"><i class="feather-log-in mr-2"></i><span class="d-none d-lg-inline">Masuk</span></a>
                    </li>
                    <li class="nav-item">
                            <a class="nav-link" href="/register"><i class="feather-edit-3 mr-2"></i><span class="d-none d-lg-inline">Daftar</span></a>
                    </li>
                {{/if}}
            </ul>
         </div>
      </nav>
      <div class="py-4">
         <div class="container">
            <div class="row">
               <!-- Main Content -->
               <main class="col col-xl-12 order-xl-12 col-lg-12 order-lg-12 col-md-12 col-sm-12 col-12">
                  <div class="box shadow-sm border rounded bg-white mb-3 osahan-share-post">
                     <h5 class="pl-3 pt-3 pr-3 border-bottom mb-0 pb-3">Cari dan Temukan UMKM Lain</h5>
                     <br/>
                     <form action="/auth/connect" method="POST">
                        <div class="col-md-4 mb-3">
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                    <span class="input-group-text" id="inputGroupPrepend3"><i class="feather-search"></i></span>
                                    </div>
                                    <input type="text" class="form-control" id="validationServerUsername" placeholder="Cari nama UMKM" name="umkm">
                                    <span><button class="btn btn-danger" type="submit">Cari</button></span>
                                </div>
                            </div>
                    </form>
                     <div class="tab-content" id="myTabContent">
                        <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                           <div class="p-3">
                              <div class="row">
                                {{#each temu}}
                                 <div class="col-md-4">
                                       <div class="border network-item rounded mb-3">
                                          <div class="p-3 d-flex align-items-center network-item-header">
                                             <div class="dropdown-list-image mr-3">
                                                <img class="rounded-circle" src="/images/profile/{{photo}}" alt="">
                                             </div>
                                             <div class="font-weight-bold">
                                                <h6 class="font-weight-bold text-dark mb-0">{{name}}</h6>
                                                <div class="small text-black-50">{{nama}}</div>
                                             </div>
                                          </div>
                                          <div class="d-flex align-items-center p-3 border-top border-bottom network-item-body">
                                              <span class="text-dark">{{deskripsi}}</span>
                                          </div>
                                          <div class="network-item-footer py-3 d-flex">
                                                <div class="col-auto">
                                                    <p class="text-dark"><i class="feather-home"></i> : {{alamat}}</p>
                                                    <p class="text-dark"><i class="feather-mail"></i> : {{email}}</p>
                                                    <p class="text-dark"><i class="feather-phone"></i> : {{telepon}}</p>
                                                </div>
                                            </div>
                                        </div>
                                 </div>
                                 {{/each}}
                  </div>
               </main>
            </div>
         </div>
      </div>
      <!-- Bootstrap core JavaScript -->
      <script src="/vendor/jquery/jquery.min.js"></script>
      <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
      <!-- slick Slider JS-->
      <script type="text/javascript" src="/vendor/slick/slick.min.js"></script>
      <!-- Custom scripts for all pages-->
      <script src="/js/osahan.js"></script>
   </body>
</html>
